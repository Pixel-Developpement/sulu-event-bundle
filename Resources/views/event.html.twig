{% extends 'base.html.twig' %}

{#{% block breadcrumb %}
    {% for item in sulu_breadcrumb(uuid) %}
        <li>
            <div class="flex items-center">
                <svg class="flex-shrink-0 h-5 w-5 text-gray-300" xmlns="http://www.w3.org/2000/svg" fill="currentColor"
                     viewBox="0 0 20 20" aria-hidden="true">
                    <path d="M5.555 17.776l8-16 .894.448-8 16-.894-.448z"/>
                </svg>
                <a href="{{ sulu_content_path(item.url) }}" class="ml-4 text-sm font-medium text-gray-500 hover:text-gray-700">{{ item.title }}</a>
            </div>
        </li>
    {% endfor %}
{% endblock %}#}

{% block content %}
    <div class="max-w-7xl mx-auto">
        <nav class="flex" aria-label="Breadcrumb">
            <ol role="list" class="flex items-center space-x-4">
                <li>
                    <div>
                        <a href="#" class="text-gray-400 hover:text-gray-500">
                            <!-- Heroicon name: solid/home -->
                            <svg class="flex-shrink-0 h-5 w-5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                <path d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z" />
                            </svg>
                            <span class="sr-only">Accueil</span>
                        </a>
                    </div>
                </li>
                <li>
                    <div class="flex items-center">
                        <svg class="flex-shrink-0 h-5 w-5 text-gray-300" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true">
                            <path d="M5.555 17.776l8-16 .894.448-8 16-.894-.448z" />
                        </svg>
                        <a href="#" class="ml-4 text-sm font-medium text-gray-500 hover:text-gray-700">Événements</a>
                    </div>
                </li>
            </ol>
        </nav>
        <div class="grid-cols-3 grid gap-16">
            <div class="col-span-2 mt-8 mb-8">
                <div class="relative mb-10">
                    <div class="absolute inset-0 flex items-center" aria-hidden="true">
                        <div class="w-full border-t border-bleu"></div>
                    </div>
                    <div class="relative flex justify-start">
                            <span class="pr-6 bg-white text-2xl uppercase font-light text-gray-900">
                             {{ event.name }}
                            </span>
                    </div>
                </div>
                {% if event.endDate == null %}
                    <p>{{ event.startDate|date('d/m/Y') }}</p>
                {% else %}
                    <p>{{ "from"|trans }} {{ event.startDate|date("d/m/Y") }} {{ "to"|trans }} {{ event.endDate|date("d/m/Y") }}</p>
                {% endif %}
                <div class="item-image mb-10">
                    {% set media = sulu_resolve_media(event.image.id, 'fr') %}
                    <img class="w-full rounded-lg" src="{{ media.thumbnails['600x400'] }}" alt="" width="1310" height="873">
                </div>

                <div class="content prose max-w-none">
                    {{ event.description|raw }}
                </div>
            </div>
            <div class="space-y-8">
